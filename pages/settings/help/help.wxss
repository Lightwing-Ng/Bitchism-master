.container {
    display: flex;
    flex-direction: column;
    height: 100%;
    justify-content: space-between;
    box-sizing: border-box;
}

.logo {
    width: 300rpx;
    height: 300rpx;
    border-radius: 50%;
    border: 0.1rpx solid #0f88eb;
    position: absolute;
    top: 3rem;
}

.DeveloperInfo {
    font-family: PingFangSC-Ultralight;
    font-size: 1rem;
    color: #585858;
    align-content: left;
    padding: 0 0 0 1rem;
    margin: 0 0 0 1rem;
    border-left: 5rpx solid #0f88eb;
    position: absolute;
    top: 40%;
}

.QRCode {
    width: 160rpx;
    height: 160rpx;
    margin: 0.5rem;
    position: relative;
    margin-top: 10%;
    margin-left: 65%;
}