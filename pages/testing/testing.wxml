<!--index.wxml-->

<import src="/wxSearch/wxSearch.wxml" />
<view class="wxSearch-section">
    <view class="wxSearch-pancel">
        <input bindinput="wxSearchInput" bindfocus="wxSerchFocus" value="{{wxSearchData.value}}" bindblur="wxSearchBlur" class="wxSearch-input" placeholder="顶配辉腾" />
        <button class='wxSearch-button' bindtap='wxSearchFn'>搜索</button>
    </view>
</view>

<template is="wxSearch" data="{{wxSearchData}}" />

<view class="container">
    <image class="logo" src="/images/logo.png" mode="aspectFit" />
</view>